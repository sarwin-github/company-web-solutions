                                      <div class="col-lg-12">
                                        <input type="text" class="form-control" id="email" name='email' placeholder="Email" required>
                                      </div>
                                    </div>
                                    <div class="form-group">
                                      <label for="message" class="col-lg-2 control-label">Message:</label>
                                      <div class="col-lg-12">
                                        <textarea class="form-control" rows="3" id="message" name='message' required></textarea>
                                      </div>
                                    </div>
                                    <br>
                                    <div class="form-group">
                                      <div class="col-lg-12">
                                        <button type="reset" style="color: #ffffff; background-color: #d82d48; border-color: transparent; padding: 10px;">      Reset      </button>
                                        <button type="submit" style="color: #ffffff; background-color: #2c3e50; border-color: transparent; padding: 10px;">      Submit      </button>
                                      </div>
                                    </div>
                                </fieldset>
